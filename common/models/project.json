{
  "name": "Project",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "properties": {
    "Organization": {
      "type": "string",
      "index": true
    },
    "Name": {
      "type": "string",
      "required": true,
      "index": true
    },
    "created": {
      "type": "date"
    },
    "modified": {
      "type": "date"
    },
    "ownerId": {
      "type": "string"
    },
    "Description": {
      "type": "string"
    },
    "URL": {
      "type": "string"
    },
    "EndDate": {
      "type": "date"
    },
    "TotalFunding": {
      "type": "number"
    },
    "GovFunding": {
      "type": "number"
    },
    "ImplementingPartners": {
      "type": [
        "string"
      ]
    },
    "Beneficiaries": {
      "type": "string"
    },
    "StartDate": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "regionalData": {
      "type": "hasMany",
      "model": "RegionalData",
      "foreignKey": "projectId"
    },
    "location": {
      "type": "hasOne",
      "model": "Location",
      "foreignKey": "projectId"
    },
    "systemUser": {
      "type": "belongsTo",
      "model": "SystemUser",
      "foreignKey": "ownerId"
    },
    "contacts": {
      "type": "hasMany",
      "model": "Contact",
      "foreignKey": "projectId"
    },
    "funders": {
      "type": "hasMany",
      "model": "Funder",
      "foreignKey": "projectId",
      "through": "Contribution"
    },
    "ogranization": {
      "type": "belongsTo",
      "model": "Ogranization",
      "foreignKey": "organizationId"
    }
  },
  "acls": [],
  "methods": []
}
